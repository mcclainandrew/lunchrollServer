        <thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
			<th>Type</th>
			<th>Additional Information</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>Latitude</td>
			<td>The user's latitude</td>
			<td>String</td>
			<td>Required</td>
		</tr>
		<tr>
			<td>Longitude</td>
			<td>The user's longitude</td>
			<td>String</td>
			<td>Required</td>
		</tr>
	</tbody>
